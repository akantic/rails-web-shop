<table class="table table-striped">
  <tr>
    <th> # </th>
    <th> Order ID </th>
    <th> Created At </th>
    <th> Last update </th>
    <th> Status </th>
    <th> Subtotal </th>
    <th>  </th>
  </tr>
  <% @orders.each_with_index do |order, i| %>
    <tr>
      <th> <%= i + 1 %> </th>
      <th> <%= order.id %> </th>
      <th> <%= order.created_at.strftime("%F %T") %> </th>
      <th> <%= order.updated_at.strftime("%F %T") %> </th>
      <th> <%= order.order_status.name %> </th>
      <th> <%= order.total %> </th>
      <th> <%= link_to 'Details', order_path(order) %> </th>
    </tr>
  <% end %>
</table>